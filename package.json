{
  "name": "babel-plugin-source-map-support",
  "version": "1.0.0",
  "description": "A Babel plugin which automatically makes stack traces source-map aware",
  "repository": "chocolateboy/babel-plugin-source-map-support",
  "license": "Artistic-2.0",
  "main": "dist/plugin.js",
  "files": [
    "package.json",
    "CHANGELOG.md",
    "README.md",
    "dist/plugin.js"
  ],
  "scripts": {
    "build": "gulp build",
    "test:debug": "cross-env NODE_ENV=development npm run test",
    "test": "gulp build && ava --verbose test/*.js"
  },
  "dependencies": {
    "babel-core": "^6.4.5",
    "lodash.ismatch": "^4.4.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.1",
    "del": "^3.0.0",
    "globby": "^6.1.0",
    "grunt-babel": "^6.0.0",
    "gulp": "gulpjs/gulp.git#4.0",
    "gulp-babel": "^6.1.2",
    "root-path": "^0.2.0"
  },
  "keywords": [
    "babel-plugin",
    "source-map",
    "sourcemap",
    "source-maps",
    "sourcemaps",
