{
    "name": "babel-plugin-source-map-support",
    "version": "2.0.1",
    "description": "A Babel plugin which automatically makes stack traces source-map aware",
    "repository": "chocolateboy/babel-plugin-source-map-support",
    "license": "Artistic-2.0",
    "main": "dist/index.js",
    "files": [
        "dist/index.js"
    ],
    "scripts": {
        "prepublishOnly": "gulp clean && cross-env NODE_ENV=production npm run test",
        "test:debug": "cross-env NODE_ENV=development npm run test",
        "test": "gulp && ava --verbose"
    },
    "dependencies": {
        "@babel/core": "^7.0.0-beta.40",
        "@babel/helper-module-imports": "^7.0.0-beta.40"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.0.0-beta.40",
        "ava": "^0.25.0",
        "babel-eslint": "^8.2.1",
        "cross-env": "^5.0.1",
        "del": "^3.0.0",
        "eslint": "^4.3.0",
        "eslint-config-standard": "^11.0.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-node": "^6.0.0",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-standard": "^3.0.1",
        "globby": "^6.0.0",
        "gulp": "^4.0.0",
        "gulp-babel": "^8.0.0-beta.1",
        "gulp-changed": "^3.2.0",
        "gulp-eslint": "^4.0.2",
        "lodash": "^4.17.5",
        "root-path": "^0.2.1"
    },
    "keywords": [
        "babel-plugin",
        "source-map",
        "sourcemap",
        "source-maps",
        "sourcemaps",
        "stack-trace",
        "stacktrace",
        "stack-traces",
        "stacktraces",
        "v8"
    ]
}
